@article{Delichatsios1996,
	Abstract = {Because of recent discussions and disagreements in literature, we were urged (Pagni, 1994) to present a brief explanation for modeling and scaling gravitationally induced fluctuations observed in pool fires, as well as in buoyant flows from a pool type (finite size) source (i.e., not a virtual (zero size) origin). This work presents some general correlations for both types of these hows, compares these correlations with experiments, and predicts trends that should be observed in large scale, mass type fires for which experimental results are unavailable.},
	Author = {Delichatsios, M. A.},
	Date-Added = {2013-03-15 20:00:22 +0000},
	Date-Modified = {2013-03-15 20:00:23 +0000},
	Journal = {Combustion Science and Technology},
	Keywords = {flames},
	Pages = {355-358},
	Title = {Gravitational fluctuations in pool fires and pool buoyant flows},
	Volume = {112},
	Year = {1996}}

@article{Zhou1995,
	Abstract = {A laser Doppler velocimeter (LDV) was used in conjunction with a quiescently seeded ambient enclosure to study the mean and transient flow field induced by a 7.1-cm toluene pool fire. Radial and axial mean velocity data are presented in the form of a vector plot to elucidate the flow patterns. A component of the vorticity deduced from the velocity field shows a rapid increase near the maximum location of the visible flame interface. The results show that the mass of air set in motion by the fire is much larger than the mass that is entrained into the visible flame and the vorticity interfaces. The results highlight and need for use of a specific unambiguous definition of entrainment when comparing experimental data and correlations. The probability density functions (pdfs) of radial and axial velocities reveal both outward and inward instantaneous motion of air as well as recirculation. The outward motion is caused by expansion and the inward motion is caused by the vorticity generated by the density gradient. A net inward motion is caused leading to entrainment. The transient measurements are of value to the development of improved understanding and analyses of fire induced flows.},
	Author = {Zhou, X. C. and Gore, J. P.},
	Date-Added = {2013-03-15 20:00:22 +0000},
	Date-Modified = {2013-03-15 20:00:23 +0000},
	Journal = {Combustion and Flame},
	Number = {1-2},
	Pages = {52-60},
	Title = {Air Entrainment Flow-Field Induced by a Pool Fire},
	Volume = {100},
	Year = {1995}}

@article{Vincent1995,
	Abstract = {The risk of accidental spills and possible fires is high in the storage and handling of large quantities of flammable liquids. Such liquid pool fires ave generally buoyancy-driven and emit a large fraction of their heat release in the form of radiation. Ignition and combustion characteristics of liquid pools depend on the design parameters such as diameter, spacing, and shape of the pools. This laboratory scale study was conducted to determine the effects of these parameters on the characteristics of multiple liquid pool fires. The measurements reported include pool surface regression rate, flame height, temperature, and concentrations of carbon dioxide, soot, and oxygen.},
	Author = {Vincent, J. R. and Gollahalli, S. R.},
	Date-Added = {2013-03-15 20:00:22 +0000},
	Date-Modified = {2013-03-15 20:00:23 +0000},
	Journal = {Journal of Energy Resources Technology-Transactions of the Asme},
	Number = {1},
	Pages = {37-42},
	Title = {An Experimental-Study of the Interaction of Multiple Liquid Pool Fires},
	Volume = {117},
	Year = {1995}}

@article{Tewarson1995,
	Abstract = {Fire properties for model based assessments of hazards and protection needs, and test apparatuses and methods used for their quwtification are discussed.
The fire properties discussed are: 1) Critical Heat Flux (CHF), 2) Thermal Response Parameter (TRP), 3) surface re-radiation loss, 4) heat of gasification, 5) flame heat flux limit, 6) yields of products, 7) heats of combustion, 8) Corrosion Index (CI), 9) Flame Extinction Index, and 10) Fire Propagation Index (FPI).
The apparatuses discussed are: 1) the Ohio State University (OSU) Heat Release Apparatus, designed by the Ohio State University, 2) the Flammability Apparatus, designed by the Factory Mutual Research Corporation (FMRC), and 3) the Cone Calorimeter, designed by the National Institute of Standards and Technology.
The test methods discussed are identified as: 1) the Ignition Test Method, 2) the Combustion Test Method (nonflaming: pyrolysis/ smoldering and flaming), 3) the Fire Propagation Test Method, and 4) the Flame Extinction Test Method.},
	Author = {Tewarson, A.},
	Date-Added = {2013-03-15 20:00:22 +0000},
	Date-Modified = {2013-03-15 20:00:23 +0000},
	Journal = {Fire and Polymers Ii},
	Keywords = {smoke heat},
	Pages = {450-497},
	Title = {Fire Properties of Materials for Model-Based Fire Assessments for Hazards and Protection Needs},
	Volume = {599},
	Year = {1995}}

@article{Sinai1995,
	Abstract = {This paper reports validation of field modelling, using the CFDS-FLOW3D code, against experimental data on the flame geometry above a round 20m kerosene pool subject to a cross-wind. The work has investigated the roles of the pool shape, the bund, and ambient turbulence, and has found that all of these parameters are capable of affecting the plume shape significantly. The predicted flame shape compares well with the observations when the modelled pool shape is similar to that of the experimental facility, and the study has shed new light on the aerodynamics of such fires.},
	Author = {Sinai, Y. L. and Owens, M. P.},
	Date-Added = {2013-03-15 20:00:22 +0000},
	Date-Modified = {2013-03-15 20:00:23 +0000},
